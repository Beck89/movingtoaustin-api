{
  "name": "mls-grid-backend",
  "version": "1.0.0",
  "description": "MLS Grid v2 Backend with ETL, API, and Search",
  "type": "module",
  "scripts": {
    "dev:api": "tsx watch api/src/index.ts",
    "dev:etl": "tsx watch etl/src/index.ts",
    "build": "tsc -b",
    "db:migrate": "tsx api/db/migrate.ts",
    "setup:meilisearch": "tsx scripts/setup-meilisearch.ts",
    "test:import": "tsx scripts/test-import.ts",
    "test:search": "tsx scripts/test-search.ts",
    "lint": "eslint . --ext .ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.701.0",
    "meilisearch": "^0.41.0",
    "dayjs": "^1.11.13",
    "express": "^4.18.2",
    "p-queue": "^7.4.1",
    "p-retry": "^6.0.0",
    "pg": "^8.12.0",
    "undici": "^6.19.8",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/pg": "^8.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}